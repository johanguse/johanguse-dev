    "@svgr/webpack": "^8.1.0",
    "cross-env": "^7.0.3",
    "dedent": "^1.5.0",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "file-loader": "^6.2.0",
    "globby": "^14.0.0",
    "husky": "^9.0.0",
    "inquirer": "^10.0.0",
    "lint-staged": "^15.2.0",
    "next-remote-watch": "^1.0.0",
    "prettier": "^3.2.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
