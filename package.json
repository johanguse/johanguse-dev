    "autoprefixer": "^10.4.4",
    "esbuild": "^0.25.0",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "mdx-bundler": "^9.0.0",
    "next": "^14.2.32",
    "next-themes": "^0.2.1",
    "postcss": "^8.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-citation": "^2.3.1",
    "rehype-slug": "^6.0.0",
    "remark-footnotes": "^4.0.1",
    "remark-gfm": "^4.0.0",
    "sharp": "^0.34.5",
    "sugar-high": "^0.9.5",
    "tailwindcss": "^3.4.0",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
